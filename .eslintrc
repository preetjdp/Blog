{
	"extends": ["next", "next/core-web-vitals", "prettier", "plugin:react/recommended", "plugin:@typescript-eslint/recommended", "plugin:prettier/recommended"],
	"plugins": [
		"simple-import-sort"
	  ],
	"rules": {
		"react/no-unescaped-entities": "off",
		"react/no-deprecated": "off"
	},
	"overrides": [
		{
		  "files": [
			"**/*.tsx"
		  ],
		  "rules": {
			"react/prop-types": "off"
		  }
		}
	  ]
}
